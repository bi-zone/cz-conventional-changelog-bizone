{
  "name": "cz-conventional-changelog-bizone",
  "version": "1.0.2",
  "description": "Commitizen adapter following the conventional-changelog format.",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "format": "prettier --write *.js",
    "cm": "npx git-cz",
    "cm:retry": "npx git-cz --retry",
    "semantic-release:dry:noci": "semantic-release --dry-run --no-ci",
    "semantic-release:dry": "semantic-release --dry-run",
    "semantic-release": "semantic-release"
  },
  "homepage": "https://ghttps://github.com/bi-zone/cz-conventional-changelog-bizone",
  "repository": {
    "type": "git",
    "url": "https://github.com/bi-zone/cz-conventional-changelog-bizone.git"
  },
  "engineStrict": true,
  "engines": {
    "node": "13.9.0"
  },
  "author": "Jim Cummins. fork by Sergio Nikolaev <jimthedev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "2.4.1",
    "word-wrap": "1.0.3",
    "current-git-branch": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.5",
    "@semantic-release/npm": "7.0.5",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "babel-eslint": "^10.1.0",
    "chai": "4.2.0",
    "commitizen": "4.0.4",
    "commitlint-config-jira": "1.4.1",
    "commitlint-plugin-jira-rules": "1.4.0",
    "cosmiconfig": "5.2.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-functional": "3.0.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-redux-saga": "1.1.3",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-xss": "0.1.10",
    "husky": "4.2.5",
    "mocha": "7.1.1",
    "mock-require": "3.0.3",
    "prettier": "2.0.2",
    "semantic-release": "17.0.4",
    "semver": "6.2.0"
  },
  "optionalDependencies": {
    "@commitlint/load": ">6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  }
}
